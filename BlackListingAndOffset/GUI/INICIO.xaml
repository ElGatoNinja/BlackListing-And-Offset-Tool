<Window x:Class="ZOT.BLnOFF.GUI.INICIO"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZOT.BLnOFF.GUI"
        mc:Ignorable="d"
        WindowStyle="ToolWindow" Background="#FFCBEAEE"
        WindowStartupLocation="CenterScreen"
        Width="300" Height="120"
        Title="ZOT" 
        TextBlock.TextAlignment="Center"
        
        Closing="INICIO_Closing"
        >


    <StackPanel Orientation="Vertical" VerticalAlignment="Center" Height="56" Margin="0,19,0,15" >
        <Image Source="/resources/logo-zelenza.png"  Width="200" Height="32" Margin="5"/>
        <ProgressBar x:Name="pbInicio" Value="0" Minimum="0" Maximum="200" IsIndeterminate="True" Background="#FFDEFD1A" BorderBrush="#FF3318E2" Foreground="#FF7A11E2" OpacityMask="#FF512EF0" Margin="45,0,0,0" HorizontalAlignment="Left" Width="200" />


    </StackPanel>



</Window>
